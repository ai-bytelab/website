---
import Layout from '../layouts/Layout.astro';
import ContactBar from '../components/ContactBar.astro';
import Navbar from '../components/Navbar.astro';
import Footer from '../components/Footer.astro';
---

<Layout title="ByteLab">

		<header class="main-header bg-white">
			<ContactBar />
			<Navbar />
		</header>

		<main id="content" class="content">
			<section class="vc_row pt-100 pb-110 bg-cover" data-parallax="true"
				data-parallax-options='{"parallaxBG": true}'
				data-row-bg="/images/wepik-export-20230426182842.png">
				<span class="row-bg-loader"></span>

				<div class="container" id="services">
					<div class="row">
						<div class="lqd-column col-md-8">
							<h2 class="text-white text-uppercase ltr-sp-1 font-weight-extrabold mb-4">
								We convert physical Documents into digital
							</h2>
							<p class="font-size-22 lh-16 pr-md-9 mr-md-9 mb-40" style="color: #bef8df;">
								We utilize advanced technologies such as Artificial
								Intelligence and Machine Learning to digitize and centralize various kinds
								of data to facilitate intelligent search and analytics.
							</p>

							<a href="#wcs" target=""
								class="btn btn-solid round font-size-12 font-weight-bold ltr-sp-2 text-uppercase px-2">
								<span>
									<span class="btn-txt">Discover ByteLab</span>
									<span class="btn-icon">
										<i class="fa fa-angle-right"></i>
									</span>
								</span>
							</a>
						</div>
						<!-- /.lqd-column col-md-8 -->
					</div>
					<!-- /.row -->
				</div>
				<!-- /.container -->
			</section>

			<section id="about" class="vc_row pt-110 pb-65">
				<div class="container">
					<div class="row" data-custom-animations="true"
						data-ca-options='{"triggerHandler":"inview","animationTarget":".lqd-column","duration":"1200","delay":"120","easing":"easeOutQuint","direction":"forward","initValues":{"translateX":30,"opacity":0},"animations":{"translateX":0,"opacity":1}}'>
						<div class="lqd-column col-md-3 col-sm-6">
							<div class="iconbox iconbox-heading-xs iconbox-xl">
								<div class="iconbox-icon-wrap">
									<span class="iconbox-icon-container mb-30">
										<i class="icon-basic_webpage_multiple"></i>
									</span>
								</div>
								<!-- /.iconbox-icon-wrap -->

								<div class="contents">
									<h3 class="mb-15">Digitization of Physical Documents</h3>
									<p>
										We streamline businesses by digitizing physical documents
										into searchable digital formats. Eliminate manual data
										entry and physical file searches, saving time and effort.
									</p>
								</div>
								<!-- /.contents -->
							</div>
							<!-- /.iconbox -->
						</div>
						<!-- /.lqd-column col-md-3 -->

						<div class="lqd-column col-md-3 col-sm-6">
							<div class="iconbox iconbox-heading-xs iconbox-xl">
								<div class="iconbox-icon-wrap">
									<span class="iconbox-icon-container mb-30">
										<i class="icon-ecommerce_bag_plus"></i>
									</span>
								</div>
								<!-- /.iconbox-icon-wrap -->

								<div class="contents">
									<h3 class="mb-15">Centralized Data Management</h3>
									<p>
										AI/ML centralize varied data types (PDFs, docs, invoices,
										receipts, handwritten docs) to optimize data workflows for
										businesses & reduce data loss risk.
									</p>
								</div>
								<!-- /.contents -->
							</div>
							<!-- /.iconbox -->
						</div>
						<!-- /.lqd-column col-md-3 -->

						<div class="lqd-column col-md-3 col-sm-6">
